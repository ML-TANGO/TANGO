    "@web/rollup-plugin-import-meta-assets": "^1.0.7",
    "babel-plugin-template-html-minifier": "^4.1.0",
    "buffer": "^6.0.3",
    "concurrently": "^7.3.0",
    "deepmerge": "^4.2.2",
    "eslint": "^8.22.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-lit-a11y": "^2.2.2",
    "eslint-plugin-wc": "^1.3.2",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
