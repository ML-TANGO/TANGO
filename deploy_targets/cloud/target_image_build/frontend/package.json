{
  "name": "forklift",
  "description": "Frontend for Forklift",
  "license": "LGPL-3.0-or-later",
  "author": "Lablup Inc.",
  "version": "0.0.0",
  "scripts": {
    "lint": "./node_modules/eslint/bin/eslint.js src -c ./.eslintrc.json  --quiet --ext .js,.ts; exit 0",
    "lint-fix": "./node_modules/eslint/bin/eslint.js src -c ./.eslintrc.json  --quiet --ext .js,.ts --fix;exit 0",
    "format": "eslint --ext .ts,.tsx . --fix --ignore-path .gitignore && prettier \"**/*.ts\" --write --ignore-path .gitignore",
    "build": "rimraf dist && tsc && npm run copyconfig && rollup -c rollup.config.js && npm run analyze -- --exclude dist",
    "analyze": "cem analyze --litelement",
    "start": "tsc && concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"wds\" --compatibility min",
    "copyconfig": "mkdir -p configs && cp ../forklift.toml configs"
  },
  "dependencies": {
    "@material/mwc-button": "^0.27.0",
    "@material/mwc-circular-progress": "^0.27.0",
    "@material/mwc-dialog": "^0.27.0",
    "@material/mwc-drawer": "^0.27.0",
    "@material/mwc-formfield": "^0.27.0",
    "@material/mwc-icon-button": "^0.27.0",
    "@material/mwc-icon-button-toggle": "^0.27.0",
    "@material/mwc-list": "^0.27.0",
    "@material/mwc-select": "^0.27.0",
    "@material/mwc-snackbar": "^0.27.0",
    "@material/mwc-switch": "^0.27.0",
    "@material/mwc-tab-bar": "^0.27.0",
    "@material/mwc-textarea": "^0.27.0",
    "@material/mwc-textfield": "^0.27.0",
    "@material/mwc-top-app-bar-fixed": "^0.27.0",
    "@microsoft/fetch-event-source": "^2.0.1",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/estree": "^1.0.0",
    "@types/hammerjs": "^2.0.41",
    "@vaadin/vaadin-grid": "^23.1.6",
    "@vanillawc/wc-codemirror": "^2.1.0",
    "@webcomponents/webcomponentsjs": "^2.6.0",
    "chart.js": "^3.9.1",
    "fs": "^0.0.1-security",
    "lit": "^2.3.1",
    "lit-element-router": "^2.0.3",
    "lit-translate": "^2.0.1",
    "markty-toml": "^0.1.1",
    "weightless": "^0.0.37"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.10",
    "@custom-elements-manifest/analyzer": "^0.6.4",
    "@open-wc/building-rollup": "^2.0.2",
    "@open-wc/eslint-config": "^8.0.2",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
