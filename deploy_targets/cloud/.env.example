# Cloud Manager Configuration
CLOUD_MANAGER_DEBUG=1
CLOUD_MANAGER_PORT=8890

# Google Cloud Platform (GCP) Configuration
GOOGLE_APPLICATION_CREDENTIALS=/source/cloud_manager/service-account-file.json
GCP_REGION=asia-northeast3
GCP_PROJECT_ID=backendai-testbed

# AWS Configuration
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_DEFAULT_REGION=ap-northeast-2

# Compute Session Configuration
CS_ENDPOINT=https://api.example.ai
CS_ACCESS_KEY=AKIA...
CS_SECRET_KEY=your-secret-key
CS_DEFAULT_DOMAIN=default
CS_DEFAULT_GROUP=default

# Image Builder Configuration (optional)
IMAGE_BUILDER_URL=http://127.0.0.1:7077
