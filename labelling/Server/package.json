{
	"name": "BluAI",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"clean": "rm -rf ./dist && rm -rf ./dist_tmp && mkdir ./dist && mkdir ./dist_tmp",
		"cpfile": "cp -r ./dist_tmp/config ./dist && cp -r ./dist_tmp/routes/sqlMapper ./dist/routes && rm -rf ./dist_tmp",
		"obfuscate": "./node_modules/javascript-obfuscator/bin/javascript-obfuscator dist_tmp --output dist",
		"build": "npm run clean && babel src -d ./dist_tmp --copy-files && npm run obfuscate && npm run cpfile",
		"dev": "npm run clean && nodemon --exec babel-node ./src/index",
		"prod": "NODE_ENV=production node ./dist/index.js",
		"master": "NODE_ENV=master node ./dist/index.js",
		"jogoon": "npm run clean && NODE_ENV=jogoon nodemon --exec babel-node ./src/index",
		"master2": "npm run clean && NODE_ENV=master nodemon --exec babel-node ./src/index"
	},
	"nodemonConfig": {
		"events": {
			"crash": "sh -c 'lsof -i :${PORT:-10236} -t | xargs kill'"
		}
	},
